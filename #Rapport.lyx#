\begin_body

\begin_layout Title
Rapport du projet de mathématiques.
\end_layout

\begin_layout Section
Méthodes directes
\end_layout

\begin_layout Subsection
Méthode triangulaire inférieure
\end_layout

\begin_layout Standard
On cherche a résoudre 
\begin_inset Formula $AX=B$
\end_inset

, pour une matrice de taille 
\begin_inset Formula $N=3$
\end_inset

, on a :
\end_layout

\begin_layout Standard
\begin_inset Formula $\left(\begin{array}{ccc}
a_{11} & 0 & 0\\
a_{21} & a_{22} & 0\\
a_{31} & a_{32} & a_{33}
\end{array}\right)\left(\begin{array}{c}
x_{1}\\
x_{2}\\
x_{3}
\end{array}\right)=\text{\left(\begin{array}{c}
b_{1}\\
b_{2}\\
b_{3}
\end{array}\right)\ensuremath{\Leftrightarrow\left\{ \begin{array}{c}
a_{11}x_{1}=b_{1}\\
a_{21}x_{1}+a_{22}x_{2}=b_{2}\\
a_{31}x_{1}+a_{32}x_{2}+a_{33}x_{3}=b_{3}
\end{array}\right.}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\Leftrightarrow\left\{ \begin{array}{c}
x_{1}=\frac{b_{1}}{a_{11}}\\
x_{2}=\frac{b_{2}-a_{21}x_{1}}{a_{22}}=\frac{b_{2}}{a_{22}}-\frac{a_{21}}{a_{22}}x_{1}\\
x_{3}=\frac{b_{3}}{a_{33}}-\frac{a_{31}}{a_{33}}x_{1}-\frac{a_{32}}{a_{33}}x_{2}=\frac{b_{3}=\sum_{k=1}^{2}a_{3k}x_{k}}{a_{33}}
\end{array}\right.$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $x_{i}=\frac{b_{i}-\sum_{j=1}^{i-1}a_{ij}x_{j}}{a_{ii}}$
\end_inset

, 
\begin_inset Formula $i=1,\ldots,N$
\end_inset

.
\end_layout

\begin_layout Subsubsection*
Algorithme
\end_layout

\begin_layout Standard
Soit A une matrice carrée de taille N, X et B un vecteur de taille N.
\end_layout

\begin_layout Standard
pour i = 1 à N faire
\end_layout

\begin_layout Standard
sum = 0
\end_layout

\begin_layout Standard
pour j=1 à i - 1 faire
\end_layout

\begin_layout Standard
sum=sum+A[i][j]*x[j]
\end_layout

\begin_layout Standard
fin pour
\end_layout

\begin_layout Standard
x[i] = (b[i] - sum)/Aii
\end_layout

\begin_layout Standard
fin pour
\end_layout

\begin_layout Subsection
Méthode triangulaire supérieure
\end_layout

\begin_layout Subsection
Elimination de Gauss
\end_layout

\begin_layout Standard
On cherche a résourde 
\begin_inset Formula $AX+B$
\end_inset

, on va chercher à les transformer l'expression en 
\begin_inset Formula $UX=$
\end_inset

 
\end_layout

\begin_layout Subsection
Factorisation LU
\end_layout

\begin_layout Standard
\begin_inset Formula $AX=B\Leftrightarrow L\underbrace{UX}_{Y}=B$
\end_inset


\end_layout

\begin_layout Standard
Donc 
\begin_inset Formula $A=LU$
\end_inset

 avec 
\begin_inset Formula $L=\begin{pmatrix} & 0 & 0\\
\vdots &  & 0\\
 & \cdots
\end{pmatrix}$
\end_inset

 et 
\begin_inset Formula $U=\begin{pmatrix}. & . & .\\
0 & . & .\\
0 & 0 & .
\end{pmatrix}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $LY=B\Rightarrow Y=\text{SolSup}\left(L,B\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $UX=Y\Rightarrow X=\text{Solsup }$
\end_inset


\end_layout

\begin_layout Subsection
Méthode de Chowleski
\end_layout

